veEventListener(\"click\", onBackDropClick);\r\n}\r\n\r\nfunction onBackDropClick(e) {\r\n  if (e.target.nodeName !== \"IMG\") {\r\n    onCloceModal(e);\r\n  }\r\n}\r\n\r\nfunction onPressEsc(e) {\r\n  if (e.key === \"Escape\") {\r\n    onCloceModal(e);\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"d30102ccc612d73c48288edfc71da009","cacheData":{"env":{}}}